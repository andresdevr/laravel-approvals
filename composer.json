{
    "name": "andredevr/laravel-approvals",
    "keywords": [
        "laravel",
        "approvals",
        "models",
        "pending changes"
    ],
    "type": "package",
    "authors": [{
        "name": "andresdevr",
        "email": "andres22.dev@gmail.com",
        "homepage": "https://github.com/andresdevr"
    }],
    "description": "This package allows to add to pending Laravel model attributes",
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Andresdevr\\LaravelApprovals\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Andresdevr\\LaravelApprovals\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.20"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Andresdevr\\LaravelApprovals\\ApprovalsServiceProvider"
            ],
            "aliases": []
        }
    }
}